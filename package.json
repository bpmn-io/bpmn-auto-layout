{
  "name": "bpmn-auto-layout",
  "version": "0.5.0",
  "description": "Layout BPMN diagrams, generating missing DI information",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "all": "run-s lint test",
    "lint": "eslint .",
    "test": "mocha",
    "pretest": "run-s build",
    "build": "rollup -c",
    "prepare": "run-s build",
    "start": "webpack serve --open --mode=development --node-env development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bpmn-io/bpmn-auto-layout.git"
  },
  "keywords": [
    "bpmn",
    "layout"
  ],
  "author": {
    "name": "bpmn.io contributors",
    "url": "https://github.com/bpmn-io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bpmn-io/bpmn-auto-layout/issues"
  },
  "homepage": "https://github.com/bpmn-io/bpmn-auto-layout#readme",
  "dependencies": {
    "bpmn-moddle": "^9.0.1",
    "min-dash": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "babel-loader": "^9.2.1",
    "bpmn-js": "^17.11.1",
    "css-loader": "^7.1.2",
    "eslint": "^8.41.0",
    "eslint-plugin-bpmn-io": "^1.0.0",
    "file-drops": "^0.5.0",
    "file-open": "^0.1.1",
    "html-webpack-plugin": "^5.6.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "rollup": "^4.12.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "engines": {
    "node": ">= 18"
  }
}
