{
  "name": "bpmn-auto-layout",
  "version": "0.4.0",
  "description": "Layout BPMN diagrams, generating missing DI information",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "all": "run-s lint test build",
    "lint": "eslint .",
    "test": "mocha --experimental-specifier-resolution=node",
    "build": "rollup -c",
    "prepare": "run-s build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bpmn-io/bpmn-auto-layout.git"
  },
  "keywords": [
    "bpmn",
    "layout"
  ],
  "author": {
    "name": "bpmn.io contributors",
    "url": "https://github.com/bpmn-io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bpmn-io/bpmn-auto-layout/issues"
  },
  "homepage": "https://github.com/bpmn-io/bpmn-auto-layout#readme",
  "dependencies": {
    "bpmn-moddle": "^9.0.0",
    "min-dash": "^4.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.1.0",
    "eslint": "^8.41.0",
    "eslint-plugin-bpmn-io": "^1.0.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.12.1"
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.0.0"
  }
}
